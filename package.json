{
  "name": "save-return-lookup-ui",
  "version": "1.0.0",
  "description": "A UI that looks up save and return items off a SQL table",
  "main": "server.js",
  "engines": {
    "node": "^14.15.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "hof-build watch",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "test:snyk": "snyk config set api=SNYK_TOKEN && snyk test",
    "db:setup": "yarn run db:migrate && yarn run db:seed",
    "db:test": "yarn run db:setup && yarn run db:rollback",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "build": "hof-build",
    "postinstall": "hof-build"
  },
  "author": "Sulthan Ahmed & Alex Swann",
  "license": "MIT",
  "dependencies": {
    "hof": "^19.8.0",
    "jquery": "^3.5.1",
    "knex": "^0.21.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "node-json-logger": "^0.0.17",
    "notifications-node-client": "^5.1.0",
    "pg": "^8.3.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-hof": "^1.2.1",
    "snyk": "^1.696.0"
  },
  "resolutions": {
    "underscore": "^1.12.1"
  }
}
